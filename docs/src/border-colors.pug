extends /_includes/_layouts/core

block config
  - pageTitle = "Border Colors"

block content
  .flrw
    each value, key in data.tailwind.theme.colors
      - className = `border-${key}`;
      .fln.p-1.flcnw.w-25p.flcch
        .h-10.w-10.border-1(class=className data-screenshot-id=className)
        code.code=`.${className}`
